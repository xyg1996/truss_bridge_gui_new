DEBUT()


mesh = LIRE_MAILLAGE(FORMAT='MED', UNITE=20)

mesh = MODI_MAILLAGE(
    reuse=mesh, MAILLAGE=mesh, ORIE_PEAU_3D=_F(GROUP_MA=('road', ))
)

model0 = AFFE_MODELE(
    AFFE=(
        _F(GROUP_MA=('road', ), MODELISATION=('DKT', ), PHENOMENE='MECANIQUE'),
        _F(
            GROUP_MA=('all_beams', ),
            MODELISATION=('POU_D_E', ),
            PHENOMENE='MECANIQUE'
        )
    ),
    MAILLAGE=mesh
)

elempro0 = AFFE_CARA_ELEM(
    COQUE=_F(EPAIS=0.2, GROUP_MA=('road', )),
    MODELE=model0,
    POUTRE=(
        _F(
            CARA=('H', ),
            GROUP_MA=('top_beams', ),
            SECTION='RECTANGLE',
            VALE=(0.1, )
        ), _F(
            CARA=('H', ),
            GROUP_MA=('main_beams', ),
            SECTION='RECTANGLE',
            VALE=(0.1, )
        ), _F(
            CARA=('H', ),
            GROUP_MA=('bottom_beams', ),
            SECTION='RECTANGLE',
            VALE=(0.1, )
        ), _F(
            CARA=('H', ),
            GROUP_MA=('lateral_beams', ),
            SECTION='RECTANGLE',
            VALE=(0.1, )
        )
    )
)

steel = DEFI_MATERIAU(ELAS=_F(E=2e11, NU=0.3, RHO=7850))
concrete = DEFI_MATERIAU(ELAS=_F(E=2.5e10, NU=0.2, RHO=2300))

fieldmat = AFFE_MATERIAU(
    AFFE=(
        _F(GROUP_MA=('all_beams', ), MATER=(steel, )),
        _F(GROUP_MA=('road', ), MATER=(concrete, ))
    ),
    MODELE=model0
)

load = AFFE_CHAR_MECA(
    DDL_IMPO=(
        _F(DX=0.0, DY=0.0, DZ=0.0, GROUP_NO=('left', )),
        _F(DY=0.0, DZ=0.0, GROUP_NO=('right', ))
    ),
    MODELE=model0,
    PRES_REP=_F(GROUP_MA=('road', ), PRES=1000)
)

reslin = MECA_STATIQUE(
    CARA_ELEM=elempro0,
    CHAM_MATER=fieldmat,
    EXCIT=_F(CHARGE=load),
    MODELE=model0
)

reslin = CALC_CHAMP(
    reuse=reslin,
    CARA_ELEM=elempro0,
    CHAM_MATER=fieldmat,
    CONTRAINTE=('EFGE_NOEU', 'SIPO_NOEU'),
    FORCE=('REAC_NODA', ),
    MODELE=model0,
    RESULTAT=reslin
)
IMPR_RESU(FORMAT='MED', RESU=_F(RESULTAT=reslin), UNITE=80)
FIN()





















FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    location    "0";
    object      pointDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    back_wall
    {
        type            fixedValue;
        value           $internalField;
    }

    front_wall
    {
        type            fixedValue;
        value           $internalField;
    }

    tank_wall
    {
        type            fixedValue;
        value           $internalField;
    }
    
    baffle_wall
    {
        type            fixedValue;
        value           $internalField;
    }
    
}

// ************************************************************************* //

FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    back_wall
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
    baffle_wall
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
    front_wall
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
    tank_wall
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
}


